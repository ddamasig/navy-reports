	<queryString language="SQL">
		<![CDATA[SELECT SUM(amount), (SELECT name FROM expense_classes WHERE id = 1) as name
FROM activity_items
WHERE expense_class_id IN
      (WITH RECURSIVE children AS (SELECT id, parent_id, deleted_at FROM expense_classes ec1 WHERE ec1.id = 1
                                   UNION ALL
                                   SELECT ec.id, ec.parent_id, ec.deleted_at
                                   FROM expense_classes ec,
                                        children ch
                                   WHERE ec.parent_id = ch.id)
      SELECT children.id FROM children WHERE deleted_at ISNULL
                                         AND parent_id IS NOT NULL)
  AND deleted_at ISNULL
  AND year = 2019
  AND activity_id IN (SELECT oa.activity_id FROM objective_activities oa WHERE oa.year = 2019)]]>
	</queryString>
